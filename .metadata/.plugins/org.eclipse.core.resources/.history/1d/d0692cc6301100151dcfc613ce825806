package hw3;

public class P464E5 {
	private String description;
	private int rating;
	
	public P464E5(String description) {
		this.description = description;
		this.rating = 0;
	}
	
	private boolean isValid(int rating) {
		if (rating > 10 || rating < 1) {
			return false;
		} else {
			return true;
		}
	}
	
	
}
