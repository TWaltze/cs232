package hw4;

public class Division implements Command {
	double[] numbers;
	
	public Division(double[] numbers) {
		this.numbers = numbers;
	}
	public void execute() {
		double result = 0;
		for(int i = 0; i < numbers.length; i++) {
			result = result / numbers[i];
		}
		
		System.out.println(result);
	}
}
